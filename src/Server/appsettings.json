{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Blog": {
    "ConnectionString": "Data Source=MicroBlog.db",
    "UseInMemory": "true",
    "UseSqLite": "false"
  },
  "JWTSettings": {
    "securityKey": "MicroBlogSecretKey",
    "validIssuer": "MicroBlogAPI",
    "validAudience": "https://localhost:5001",
    "expiryInMinutes": 3
  },
  "IdentityOptions": {
    "HighComplexity": false,
    "RequireUniqueEmail": true,
    "MaxFailedAccessAttempts": 5
  }
}
