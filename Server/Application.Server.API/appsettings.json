{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",

  "Blog": {
    "UseInMemory": "true",
    "UseSqLite": "false",
    "ConnectionString": "Data Source=MicroBlog.db"
  },
  "Storage": {
    "Path": "/uploads",
    "MaxSizeLimit": 100000000
  },
  "ConnectionStrings": {
    "Default": "Database=MicroBlog.db"
  }
}
